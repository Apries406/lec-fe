:root[dark], .force-dark {
    /* Colors / Label  */
    --color-primary-label rgb(255, 255, 255)
    --color-secondary-label rgba(255, 255, 255, 0.8)
    --color-tertiary-label rgba(255, 255, 255, 0.6)
    --color-quaternary-label rgba(255, 255, 255, 0.3)
    --color-link rgb(199, 218, 239)
    --color-inverted-label rgb(10, 10, 10)

    /* Colors / Background */
    --color-background rgb(10, 10, 10)
    --color-active-background rgba(255, 255, 255, 0.04)

    /* Colors / Border */
    --color-border rgba(255, 255, 255, 0.08)
    --color-shadow-border rgba(255, 255, 255, 0.02)

    /* Elevation / Background color */
    --elevation-low-background rgb(18, 18, 18)
    --elevation-high-background rgb(24, 24, 24)
    --elevation-note-background rgb(18, 18, 18)

    /* Elevation / Shadow */
    --elevation-low-shadow 0px 2px 8px 0px rgba(0, 0, 0, 0.04), 0px 1px 2px 0px rgba(0, 0, 0, 0.02), 0px 0px 0px 1px rgba(255, 255, 255, 0.04) inset
    --elevation-high-shadow 0px 4px 32px 0px rgba(0, 0, 0, 0.08), 0px 1px 4px 0px rgba(0, 0, 0, 0.04), 0px 0px 0px 1px rgba(255, 255, 255, 0.06) inset
    --elevation-note-shadow 0px 8px 64px 0px rgba(0, 0, 0, 0.04), 0px 1px 4px 0px rgba(0, 0, 0, 0.02)

    /* Elevation / Shadow filter 备注：Filter 版和 Shadow 是同样效果，用于不同的 CSS 属性 */
    --elevation-low-shadow-filter drop-shadow(0px 2px 8px rgba(0, 0, 0, 0.04)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.02)) drop-shadow(0px 0px 1px rgba(255, 255, 255, 0.04))
    --elevation-high-shadow-filter drop-shadow(0px 4px 32px rgba(0, 0, 0, 0.08)) drop-shadow(0px 1px 4px rgba(0, 0, 0, 0.04)) drop-shadow(0px 0px 1px rgba(255, 255, 255, 0.06))

    /* Material */
    --material-filter saturate(150%) blur(10px)
    --material-background rgba(64, 64, 64, 0.25)
    --material-inverted-background rgba(255, 255, 255, 0.9)

    /* Mask */
    --mask-backdrop rgba(0, 0, 0, 0.5)
    --mask-note-card rgba(0, 0, 0, 0.25)
    --mask-paper rgba(10, 10, 10, 0.98)

    /* Colors / Standard */
    --color-white rgb(255, 255, 255)
    --color-red rgba(255, 46, 77, 1)
    --color-tinted-red rgba(255, 46, 77, 0.06)
    --color-blue rgba(61, 138, 245, 1)
    --color-tinted-blue rgba(61, 138, 245, 0.1)
    --mask-video-player-mask linear-gradient(180deg, rgba(0, 0, 0, 0.25) 0%, rgba(0, 0, 0, 0.00) 24.48%, rgba(0, 0, 0, 0.00) 50%, rgba(0, 0, 0, 0.75) 100%)

    --search-hotspot-hint linear-gradient(90deg, rgba(255, 37, 67, 1) 0%,  rgba(255, 82, 37, 1) 100%)

    --color-vertical-channel #181714
}
